<div class="article-gallery <% if (count % 2 == 0) { %>right<% } else { %>left<% } %> <% if (post.layout == 'sticky') { %>sticky<% } %>">
  <% if (post.photos != ''){ %>
    <% post.photos.forEach(function(photo, i){ %>
      <div class="image" style="background-image: url(<%- url_for(photo) %>);"></div>
    <% }) %>
  <% } else { %>
    <div class="image" style="background-image: url(https://picsum.photos/450/350/?random);"></div>
  <% } %>
</div>
