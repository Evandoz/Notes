<% if (post.prev || post.next){ %>
  <div class="post-paginator">
    <% if (post.prev){ %>
      <a href="<%- url_for(post.prev.path) %>" class="paginator-link"><%= __('newer') %><%= post.prev.title %></a>
    <% } %>
    <% if (post.next){ %>
      <a href="<%- url_for(post.next.path) %>" class="paginator-link"><%= __('older') %><%= post.next.title %></a>
    <% } %>
  </div>
<% } %>
