<!DOCTYPE html>
<html>
<%- partial('_partial/head') %>
<body>
  <div id="overflow" class="overflow">
    <div id="hiddenbar" class="hiddenbar">
      <div id="wrapper" class="wrapper">
        <%- partial('_partial/header') %>
        <div id="content" class="content"><%- body %></div>
        <% if (!is_home() && !is_post()) { %>
          <%- partial('_partial/footer', null, { cache: !config.relative_link }) %>
        <% } %>
      </div>
    </div>
  </div>
  <%- partial('_partial/anchor') %>
  <%- partial('_partial/mobile') %>
  <%- partial('_partial/script') %>
</body>
</html>
