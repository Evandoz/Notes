.header
  position: relative
  .header-wrap
    @media mq-mobile
      justify-content: center

.text-center
  text-align: center

  .not-found-title
    display: block
    color: #333
    font-size: 24px
    font-weight: 500

  .back-button
    display: inline-block
    margin: 30px 0
    padding: 10px 40px
    color: #fff
    font-size: 14px
    letter-spacing: 2px
    text-decoration: none
    text-transform: uppercase
    background: #333
    border: 2px solid #333

  .back-button:hover
    color: #333
    border: 2px solid #333
    background: transparent
    transition: background .3s

